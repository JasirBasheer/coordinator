  <script>
    $(document).ready(function() {
        $('.message-toggle').on('click', function() {
            $.ajax({
                url: '/admin/getCoordinators', 
                method: 'POST',
                dataType: 'json',
                success: function(response) {
                  document.getElementById('batchCount').innerText = response.coordinators.length-1
                    let content = '';
                    response.coordinators.forEach(function(item) {
                      const batch = item.batch ? item.batch.toUpperCase() : 'N/A'; 
                        content += `
                            <a href="#" class="dropdown-item">
                                <span class="dropdown-item-avatar text-white">
                                    <img alt="image" src="${item.image}" class="rounded-circle">
                                </span>
                                <span class="dropdown-item-desc">
                                    <span class="user-batch">${batch}</span>
                                    <span class="time user-name">${item.name}</span>
                                    <span class="time user-email">${item.email}</span>
                                </span>
                            </a>`;
                    });
                    $('#dropdown-list-content').html(content);
                },
                error: function(xhr, status, error) {
                    console.error('Error fetching data:', error);
                }
            });
        });
    });
</script>

